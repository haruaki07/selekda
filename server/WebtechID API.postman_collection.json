			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"try {",
							"    const response = await pm.sendRequest({",
							"        url: pm.environment.get(\"base_url\") + \"/api/captcha\",",
							"        method: \"GET\"",
							"    });",
							"",
							"    let data = response.json().data;",
							"    let answer = eval(data.challenge);",
							"",
							"    pm.environment.set(\"_captcha\", `${data.token}-${answer}`);",
							"} catch (err) {",
							"    console.error(err);",
							"}"
